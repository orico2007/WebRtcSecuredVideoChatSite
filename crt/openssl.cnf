[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
x509_extensions    = v3_req

[ dn ]
C  = IL
ST = IL
L  = IL
O  = WebRtcSecured
OU = Dev
CN = 127.0.0.1

[ v3_req ]
basicConstraints = CA:FALSE
keyUsage = critical, digitalSignature, keyEncipherment
extendedKeyUsage = serverAuth
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = localhost
IP.1 = 127.0.0.1 
IP.2 = 84.229.208.139 
IP.3 = 192.168.1.245